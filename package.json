{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "mocha test/ele.test.js"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "ele",
    "directories": {
      "output": "bin"
    },
    "win": {
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "icon.ico"
    },
    "extraResources": [
      {
          "from":"./libs",
          "to":"../libs"
      }
  ]
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^22.8.1",
    "mocha": "^5.0.1"
  },
  "dependencies": {
    "ffi-napi": "^2.4.5"
  }
}
